        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ComposeExample"
        tools:targetApi="31">
        <activity
            android:name=".EffectHandlersEx"
            android:exported="true"
            android:label="@string/title_activity_effect_hanlders_ex"
            android:theme="@style/Theme.ComposeExample">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ConstraintLayoutEx"
            android:exported="false"
            android:label="@string/title_activity_constraint_layout_ex"
            android:theme="@style/Theme.ComposeExample" />

        <activity
