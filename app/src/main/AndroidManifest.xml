            android:label="@string/title_activity_snack_bar_sample"
            android:theme="@style/Theme.ComposeExample" />
        <activity
            android:name=".StateComposeActivity"
            android:exported="false"
            android:label="@string/title_activity_state_compose"
            android:theme="@style/Theme.ComposeExample" />
        <activity
            android:name=".TextActivity"
            android:exported="false"
            android:label="@string/title_activity_text"
            android:theme="@style/Theme.ComposeExample" />
        <activity
            android:name=".MainActivity"
            android:exported="false"
            android:label="@string/app_name"
            android:theme="@style/Theme.ComposeExample" />
    </application>

</manifest>